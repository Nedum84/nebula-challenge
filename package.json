{
  "name": "iclass-api",
  "version": "1.0.0",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "NODE_ENV=local ts-node -r tsconfig-paths/register src/index.ts",
    "dev": "NODE_ENV=local ts-node-dev -r tsconfig-paths/register --poll src/index.ts",
    "start:dev": "NODE_ENV=local concurrently \"npx tsc --watch\" \"nodemon -q dist/src/index.js\"",
    "test": "jest --runInBand --no-cache",
    "test:watch": "jest --runInBand --watchAll --no-cache",
    "sls:offline": "serverless offline --stage dev",
    "sls:deploy": "serverless deploy --stage dev",
    "sls:remove": "serverless remove --stage dev",
    "sls:invoke": "serverless invoke --function api --stage dev",
    "clean": "rm -rf ./dist",
    "copy": "sh ./scripts/copy.sh",
    "build": "npm run clean && tsc",
    "cli": "npm run build && cd dist && node src/cli/local.cli.js",
    "cache:cli": "NODE_ENV=local node dist/src/cli/cache.cli execute",
    "git:push": "git add . && git commit -m 'chore: more additions [skip test]' && git push",
    "dc:up": "docker compose up -d"
  },
  "description": "",
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.850.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.850.0",
    "@aws-sdk/client-dynamodb": "^3.850.0",
    "@aws-sdk/client-scheduler": "^3.848.0",
    "@aws-sdk/client-sns": "^3.848.0",
    "@aws-sdk/client-sqs": "^3.854.0",
    "@aws-sdk/lib-dynamodb": "^3.850.0",
    "body-parser": "^2.2.0",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "express-fileupload": "^1.5.2",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "http-status": "^2.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "morgan": "^1.10.1",
    "winston": "^3.17.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@codegenie/serverless-express": "^4.17.0",
    "@faker-js/faker": "^9.9.0",
    "@types/aws-lambda": "^8.10.152",
    "@types/express": "^5.0.3",
    "@types/express-fileupload": "^1.5.1",
