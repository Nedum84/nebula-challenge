{
  "name": "iclass-api",
  "version": "1.0.0",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "NODE_ENV=local ts-node -r tsconfig-paths/register src/index.ts",
    "dev": "NODE_ENV=local ts-node-dev -r tsconfig-paths/register --poll src/index.ts",
    "start:dev": "NODE_ENV=local concurrently \"npx tsc --watch\" \"nodemon -q dist/src/index.js\"",
    "test": "jest --runInBand --no-cache",
    "test:watch": "jest --runInBand --watchAll --no-cache",
    "test:userapp": "jest ./src/apps/user --runInBand --no-cache",
    "test:adminapp": "jest ./src/apps/admin --runInBand --no-cache",
    "test:specs": "jest --testPathIgnorePatterns \\(/src/apps/admin/ /src/apps/user/\\) --runInBand --no-cache",
    "sls:offline": "sls offline --stage dev",
    "clean": "rm -rf ./dist",
    "copy": "sh ./scripts/copy.sh",
    "build": "npm run clean && tsc && npm run copy",
    "cli": "npm run build && cd dist && node src/cli/local.cli.js",
    "db:drop": "npm run cli s:cli db:drop",
    "db:drop:manual": "cd dist && sequelize-cli db:drop",
    "db:create": "npm run cli s:cli db:create",
    "db:migrate": "npm run cli s:cli db:migrate",
    "db:post:migrate": "npm run cli s:cli db:post:migrate",
    "db:seed:all": "npm run cli s:cli db:seed:all",
    "db:reset": "npm run cli s:cli db:drop db:create db:migrate db:post:migrate",
    "db:reset:all": "npm run cli s:cli db:drop db:create db:migrate db:post:migrate",
    "db:migrate:all": "npm run build && npx makemigration && npm run cli s:cli db:migrate",
    "db:makemigration": "npm run build && npx makemigration",
    "db:gen:post:migration:file": "ts-node-dev src/cli/post.migration.cli s:cli:m",
    "cache:cli": "NODE_ENV=local node dist/src/cli/cache.cli execute",
    "git:push": "git add . && git commit -m 'chore: more additions [skip test]' && git push",
    "dc:up": "docker compose up -d",
    "migrate:prepare": "ts-node-dev -r tsconfig-paths/register --poll src/backup-cbt-edcr/index.ts"
  },
  "description": "",
